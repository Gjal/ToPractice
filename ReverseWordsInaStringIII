 //557.Reverse Words in a StringIII
 //reverse()
public String reverseWords(String s){
        String res="";
        String[] str=s.split(" ");
        for(int i=0;i<str.length;i++){
            if(i==str.length-1){
                res+=new StringBuffer(str[i]).reverse().toString();
            }else{
                res+=new StringBuffer(str[i]).reverse().append(" ").toString();
            }
        }
        return res;
    }
